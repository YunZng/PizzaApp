@page
@model InvalidTokenModel
@{
    ViewData["Title"] = "Invalid Token";
}
<h1>@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-4">
        <div class="alert alert-danger" role="alert">
            <p>Invalid token, please contact your invitation sender for help.</p>
        </div>
    </div>
</div>